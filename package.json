  },
  "devDependencies": {
    "@mozaik/babel-preset": "^1.0.0-alpha.1",
    "@mozaik/ui": "^2.0.0-alpha.7",
    "ava": "0.16.0",
    "babel-cli": "^6.24.1",
    "coveralls": "2.11.12",
    "cross-env": "^5.0.1",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-plugin-react": "6.1.2",
    "mockery": "1.7.0",
    "nyc": "8.1.0",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
